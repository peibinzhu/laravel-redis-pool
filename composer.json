{
    "name": "peibin/laravel-redis-pool",
    "description": "Peibin redis pool abstraction component for Laravel.",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "config-center"
    ],
    "authors": [
        {
            "name": "Peibin Zhu",
            "email": "zhupeibin1207@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "peibin/laravel-context": "^1.0",
        "peibin/laravel-pool": "^1.0",
        "peibin/laravel-swoole-event": "^1.0",
        "peibin/laravel-utils": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "PeibinLaravel\\RedisPool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeibinLaravelTest\\RedisPool\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PeibinLaravel\\RedisPool\\RedisPoolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
